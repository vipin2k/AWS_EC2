pipeline {
    agent any

    stages {
        stage('Connect to SSH Server') {
            steps {
                node {
                    // Define and use the sshagent step here
                    sshagent(credentials: ['15.206.145.189']) {
                        // Use the ssh step to run SSH commands on the remote server
                        ssh user: 'ubuntu', remote: '15.206.145.189', command: 'ls -l'
                    }
                }
            }
        }
    }
}
